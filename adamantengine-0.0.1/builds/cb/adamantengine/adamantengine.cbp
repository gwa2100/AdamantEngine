<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="adamantengine" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="..\..\..\lib\libadamantengine" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DDLL_EXPORT" />
					<Add directory="..\..\..\contrib\mingw\include\SDL" />
					<Add directory="..\..\..\contrib\mingw\include" />
					<Add directory="..\..\..\contrib\include" />
				</Compiler>
				<Linker>
					<Add library="..\..\..\contrib\mingw\lib\libSDL.dll.a" />
					<Add library="..\..\..\contrib\mingw\lib\libSDLmain.a" />
					<Add directory="..\..\..\contrib\mingw\lib" />
					<Add directory="..\..\..\contrib\lib" />
				</Linker>
				<ExtraCommands>
					<Add before="cmd.exe /c copy ..\platform.hpp ..\..\..\src" />
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="..\..\..\lib\libadamantengine" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DDLL_EXPORT" />
					<Add directory="..\..\..\contrib\mingw\include\SDL" />
					<Add directory="..\..\..\contrib\mingw\include" />
					<Add directory="..\..\..\contrib\include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="..\..\..\contrib\mingw\lib\libSDL.dll.a" />
					<Add library="..\..\..\contrib\mingw\lib\libSDLmain.a" />
					<Add directory="..\..\..\contrib\mingw\lib" />
					<Add directory="..\..\..\contrib\lib" />
				</Linker>
				<ExtraCommands>
					<Add before="cmd.exe /c copy ..\platform.hpp ..\..\..\src" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="..\..\..\src\App.cpp" />
		<Unit filename="..\..\..\src\App.hpp" />
		<Unit filename="..\..\..\src\GameObject.cpp" />
